{
  "name": "@opam/ocamlfind",
  "version": "1.7.3",
  "esy": {
    "build": [
      [
        "bash",
        "./_esy/build",
        "#{ocaml.lib / 'ocaml'}"
      ]
    ],
    "buildsInSource": true,
    "exportedEnv": {
      "OCAMLLIB": {
        "val": "#{@opam/ocamlfind.install / 'lib' / 'ocaml'}",
        "scope": "global"
      },
      "CAML_LD_LIBRARY_PATH": {
        "val": "#{@opam/ocamlfind.install / 'lib' / 'ocaml' / 'stublibs' : @opam/ocamlfind.install / 'lib' / 'ocaml' : $CAML_LD_LIBRARY_PATH}",
        "scope": "global"
      },
      "OCAML_TOPLEVEL_PATH": {
        "val": "#{@opam/ocamlfind.install / 'lib' / 'ocaml'}",
        "scope": "global"
      },
      "ocamlfind_installed": {
        "scope": "global",
        "val": "true"
      },
      "ocamlfind_version": {
        "scope": "global",
        "val": "1.7.3"
      },
      "ocamlfind_enable": {
        "scope": "global",
        "val": "enable"
      }
    }
  },
  "dependencies": {
    "substs": "esy-ocaml/substs",
    "@opam/conf-m4": "*",
    "opam-installer-bin": "esy-ocaml/opam-installer-bin"
  },
  "scripts": {},
  "peerDependencies": {
    "ocaml": ">= 3.12.0"
  }
}
